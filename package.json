{
  "name": "kuneiform-parser",
  "version": "0.1.1",
  "description": "A JavaScript / TypeScript compatible parser for the Kuneiform language. This should be used with the Kwil NPM package.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*.js",
    "dist/**/*.ts",
    "!dist/**/*.map",
    "dist/**/*.wasm",
    "!dist/**/*.test.js",
    "!dist/**/*.test.ts",
    "CHANGELOG.md",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "npm run clean && tsc",
    "release": "standard-version",
    "commit": "cz",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.3",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "axios": "^1.4.0"
  }
}
